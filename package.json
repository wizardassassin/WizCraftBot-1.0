{
  "name": "wizcraftbot-1.0",
  "version": "1.0.0",
  "description": "Discord Bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./dist/ ./node_modules/",
    "start": "pm2 start ecosystem.config.cjs",
    "stop": "pm2 stop ecosystem.config.cjs",
    "deploy-cmd": "node ./dist/deploy-commands.js",
    "deploy-prisma": "prisma migrate deploy && prisma generate",
    "test": "cd ./dist/ && node .",
    "update": "npm stop && git pull && npm run build2 && npm start",
    "build2": "npm run clean && npm ci && npm run build && npm run deploy-cmd && npm run deploy-prisma"
  },
  "author": "wizardassassin",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#utils/*": "./dist/utils/*"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.3.0",
    "@discordjs/voice": "^0.17.0",
    "@distube/ytdl-core": "^4.13.5",
    "@distube/ytsr": "^2.0.4",
    "@prisma/client": "^5.17.0",
    "bufferutil": "^4.0.8",
    "canvas": "^2.11.2",
    "chess.js": "^1.0.0-beta.8",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "erlpack": "github:discord/erlpack",
    "ffmpeg-static": "^5.2.0",
    "lodash": "^4.17.21",
    "minecraft-server-util": "^5.4.2",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.13",
    "simple-oauth2": "^5.1.0",
    "sodium-native": "^4.1.1",
    "typescript": "^5.5.3",
    "utf-8-validate": "^6.0.4",
    "ytdl-core": "^4.11.5",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.4",
    "zlib-sync": "^0.1.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizardassassin/WizCraftBot-1.0.git"
  },
  "keywords": [
    "music",
    "api",
    "bot",
    "minecraft",
    "node",
    "discord",
    "nodejs",
    "discord-bot",
    "discordjs"
  ],
  "bugs": {
    "url": "https://github.com/wizardassassin/WizCraftBot-1.0/issues"
  },
  "homepage": "https://github.com/wizardassassin/WizCraftBot-1.0#readme",
  "devDependencies": {
    "prisma": "^5.17.0"
  }
}
